{
  "name": "theme-blog-demo",
  "private": true,
  "description": "Blog demo for vuepress-theme-hope",
  "license": "MIT",
  "author": "Mr.Hope",
  "type": "module",
  "scripts": {
    "demo:serve": "http-server -a localhost dist -o",
    "demo:vite-bundle-build": "vuepress-vite build src --clean-cache --clean-temp",
    "demo:vite-custom-build": "cross-env THEME_CUSTOM=true vuepress-vite build src --clean-cache --clean-temp",
    "demo:vite-build": "pnpm demo:vite-bundle-build && pnpm demo:vite-custom-build",
    "demo:vite-custom-clean-dev": "cross-env THEME_CUSTOM=true vuepress-vite dev src --clean-cache --clean-temp",
    "demo:vite-clean-dev": "vuepress-vite dev src --clean-cache --clean-temp",
    "demo:vite-custom-dev": "cross-env THEME_CUSTOM=true vuepress-vite dev src",
    "demo:vite-dev": "vuepress-vite dev src",
    "demo:webpack-bundle-build": "vuepress-webpack build src --clean-cache --clean-temp",
    "demo:webpack-custom-build": "cross-env THEME_CUSTOM=true vuepress-webpack build src --clean-cache --clean-temp",
    "demo:webpack-build": "pnpm demo:webpack-bundle-build && pnpm demo:webpack-custom-build",
    "demo:webpack-custom-clean-dev": "cross-env THEME_CUSTOM=true vuepress-webpack dev src --clean-cache --clean-temp",
    "demo:webpack-clean-dev": "vuepress-webpack dev src --clean-cache --clean-temp",
    "demo:webpack-custom-dev": "cross-env THEME_CUSTOM=true vuepress-webpack dev src",
    "demo:webpack-dev": "vuepress-webpack dev src"
  },
  "devDependencies": {
    "@vue/repl": "4.5.1",
    "@vuepress/bundler-vite": "catalog:vuepress",
    "@vuepress/bundler-webpack": "catalog:vuepress",
    "@vuepress/helper": "catalog:vuepress",
    "@vuepress/plugin-docsearch": "catalog:vuepress",
    "@vuepress/plugin-revealjs": "catalog:vuepress",
    "@waline/client": "3.5.6",
    "chart.js": "4.4.9",
    "echarts": "5.6.0",
    "katex": "0.16.22",
    "kotlin-playground": "1.30.0",
    "markmap-lib": "catalog:",
    "markmap-toolbar": "catalog:",
    "markmap-view": "catalog:",
    "mathjax-full": "catalog:",
    "mermaid": "catalog:",
    "reveal.js": "catalog:",
    "sandpack-vue3": "3.1.12",
    "sass-embedded": "catalog:",
    "sass-loader": "catalog:",
    "vue": "catalog:",
    "vuepress": "catalog:vuepress",
    "vuepress-theme-hope": "workspace:*"
  }
}
