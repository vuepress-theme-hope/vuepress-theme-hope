{
  "name": "vuepress-theme-hope",
  "version": "2.0.0-beta.88",
  "description": "A light vuepress theme with tons of features",
  "keywords": [
    "vuepress",
    "vuepress2",
    "theme",
    "vuepress-theme",
    "blog",
    "vuepress-blog",
    "vuepress-theme-blog"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/theme"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src",
    "templates"
  ],
  "scripts": {
    "build": "pnpm build:ts && pnpm build:copy",
    "build:copy": "cpx \"src/client/**/*.{css,scss,jpg,vue}\" lib/client",
    "build:ts": "tsc -b tsconfig.release.json",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm build:copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@vuepress/cli": "2.0.0-beta.49",
    "@vuepress/client": "2.0.0-beta.49",
    "@vuepress/core": "2.0.0-beta.49",
    "@vuepress/plugin-active-header-links": "2.0.0-beta.49",
    "@vuepress/plugin-container": "2.0.0-beta.49",
    "@vuepress/plugin-external-link-icon": "2.0.0-beta.49",
    "@vuepress/plugin-git": "2.0.0-beta.49",
    "@vuepress/plugin-nprogress": "2.0.0-beta.49",
    "@vuepress/plugin-palette": "2.0.0-beta.49",
    "@vuepress/plugin-prismjs": "2.0.0-beta.49",
    "@vuepress/plugin-theme-data": "2.0.0-beta.49",
    "@vuepress/shared": "2.0.0-beta.49",
    "@vuepress/utils": "2.0.0-beta.49",
    "@vueuse/core": "^9.1.0",
    "balloon-css": "^1.2.0",
    "bcrypt-ts": "^1.0.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vuepress-plugin-blog2": "workspace:*",
    "vuepress-plugin-comment2": "workspace:*",
    "vuepress-plugin-components": "workspace:*",
    "vuepress-plugin-copy-code2": "workspace:*",
    "vuepress-plugin-copyright2": "workspace:*",
    "vuepress-plugin-feed2": "workspace:*",
    "vuepress-plugin-md-enhance": "workspace:*",
    "vuepress-plugin-photo-swipe": "workspace:*",
    "vuepress-plugin-pwa2": "workspace:*",
    "vuepress-plugin-reading-time2": "workspace:*",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-plugin-seo2": "workspace:*",
    "vuepress-plugin-sitemap2": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-beta.49",
    "@vuepress/bundler-webpack": "2.0.0-beta.49",
    "@vuepress/markdown": "2.0.0-beta.49",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "sass-loader": "^13.0.0"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
