{
  "name": "vuepress-plugin-copyright2",
  "version": "2.0.0-beta.91",
  "description": "Copyright plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "copyright"
  ],
  "homepage": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/packages/copyright#readme",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/copyright2"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./lib/client/*": "./lib/client/*",
    "./package.json": "./package.json"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.50",
    "@vuepress/shared": "2.0.0-beta.50",
    "@vuepress/utils": "2.0.0-beta.50",
    "@vueuse/core": "^9.1.1",
    "vue": "^3.2.37",
    "vuepress-shared": "workspace:2.0.0-beta.91"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.50",
    "rimraf": "3.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
