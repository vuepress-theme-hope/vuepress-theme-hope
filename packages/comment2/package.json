{
  "name": "vuepress-plugin-comment2",
  "version": "2.0.0-beta.91",
  "description": "A comment plugin for vuepress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "vuepress-blog",
    "blog",
    "comment",
    "giscus",
    "twikoo",
    "waline"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/comment/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/comment2"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./lib/client/*": "./lib/client/*",
    "./package.json": "./package.json"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.50",
    "@vuepress/utils": "2.0.0-beta.50",
    "@waline/client": "^2.6.3",
    "giscus": "^1.2.0",
    "twikoo": "^1.6.7",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5",
    "vuepress-plugin-sass-palette": "workspace:2.0.0-beta.91",
    "vuepress-shared": "workspace:2.0.0-beta.91"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.50",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "sass-loader": "^13.0.0"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
