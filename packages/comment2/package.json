{
  "name": "vuepress-plugin-comment2",
  "version": "2.0.0-beta.47",
  "description": "A comment plugin for vuepress",
  "keywords": [
    "vuepress-plugin",
    "vuepress-blog",
    "comment",
    "valine",
    "vssue",
    "page-info"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/comment/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/comment2"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@mr-hope/vuepress-shared": "workspace:*",
    "@vuepress/client": "2.0.0-beta.41",
    "@vuepress/utils": "2.0.0-beta.41",
    "@waline/client": "^2.0.3",
    "giscus": "^1.0.3",
    "twikoo": "^1.5.4",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vuepress-plugin-sass-palette": "workspace:*"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.41"
  },
  "peerDependencies": {
    "sass-loader": "^12.6.0"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
