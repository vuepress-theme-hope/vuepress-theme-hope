{
  "name": "vuepress-plugin-components",
  "version": "2.0.0-beta.242",
  "description": "Components collection plugin for VuePress2",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "components"
  ],
  "homepage": "https://plugin-components.vuejs.press/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/components"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client/*": "./lib/client/*",
    "./package.json": "./package.json"
  },
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@stackblitz/sdk": "^1.9.0",
    "@vuepress/client": "2.0.0-beta.67",
    "@vuepress/shared": "2.0.0-beta.67",
    "@vuepress/utils": "2.0.0-beta.67",
    "@vueuse/core": "^10.5.0",
    "artplayer": "^5.0.9",
    "balloon-css": "^1.2.0",
    "create-codepen": "1.0.1",
    "qrcode": "^1.5.3",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vuepress-plugin-reading-time2": "workspace:*",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/qrcode": "1.5.4",
    "@vuepress/bundler-vite": "2.0.0-beta.67",
    "@vuepress/core": "2.0.0-beta.67"
  },
  "peerDependencies": {
    "dashjs": "^4.7.2",
    "hls.js": "^1.4.12",
    "mpegts.js": "^1.7.3",
    "plyr": "^3.7.8",
    "sass-loader": "^13.3.2",
    "vidstack": "^1.4.7",
    "vuepress": "2.0.0-beta.67",
    "vuepress-vite": "2.0.0-beta.67",
    "vuepress-webpack": "2.0.0-beta.67"
  },
  "peerDependenciesMeta": {
    "dashjs": {
      "optional": true
    },
    "hls.js": {
      "optional": true
    },
    "mpegts.js": {
      "optional": true
    },
    "plyr": {
      "optional": true
    },
    "sass-loader": {
      "optional": true
    },
    "vidstack": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  }
}
