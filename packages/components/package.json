{
  "name": "@mr-hope/vuepress-plugin-components",
  "version": "2.0.0-alpha.7",
  "description": "Components plugin for vuepress-theme-hope",
  "keywords": [
    "vuepress-plugin",
    "components"
  ],
  "homepage": "https://github.com/Mister-Hope/vuepress-theme-hope/tree/v2/packages/components#readme",
  "bugs": {
    "url": "https://github.com/Mister-Hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/vuepress-theme-hope.git",
    "directory": "packages/components"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "zhangbowang1998@gmail.com",
    "url": "https://mrhope.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "demo:vite-build": "vuepress-vite build __tests__/demo",
    "demo:vite-clean-serve": "vuepress-vite dev __tests__/demo --clean-cache",
    "demo:vite-serve": "vuepress-vite dev __tests__/demo",
    "demo:webpack-build": "vuepress-webpack build __tests__/demo",
    "demo:webpack-clean-serve": "vuepress-webpack dev __tests__/demo --clean-cache",
    "demo:webpack-serve": "vuepress-webpack dev __tests__/demo",
    "dev": "concurrently \"yarn dev:copy\" \"yarn dev:ts\"",
    "dev:copy": "cpx src/client/**/*.{vue,scss} lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@mr-hope/vuepress-shared": "2.0.0-alpha.7",
    "@types/lodash.debounce": "^4.0.6",
    "@vuepress/client": "2.0.0-beta.33",
    "@vuepress/plugin-theme-data": "2.0.0-beta.33",
    "@vuepress/utils": "2.0.0-beta.33",
    "balloon-css": "^1.2.0",
    "lodash.debounce": "^4.0.8",
    "screenfull": "^6.0.1",
    "vue": "^3.2.27",
    "vue-router": "^4.0.12",
    "vuepress-plugin-sass-palette": "2.0.0-alpha.7"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.33"
  },
  "publishConfig": {
    "access": "public"
  }
}
