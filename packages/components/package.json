{
  "name": "vuepress-plugin-components",
  "version": "2.0.0-beta.143",
  "description": "Components collection plugin for VuePress2",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "components"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/components/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/components"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client/*": "./lib/client/*",
    "./package.json": "./package.json"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.59",
    "@vuepress/shared": "2.0.0-beta.59",
    "@vuepress/utils": "2.0.0-beta.59",
    "@vueuse/core": "^9.9.0",
    "balloon-css": "^1.2.0",
    "plyr": "^3.7.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuepress-plugin-reading-time2": "workspace:*",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.59"
  },
  "peerDependencies": {
    "sass-loader": "^13.2.0",
    "vuepress": "2.0.0-beta.59",
    "vuepress-vite": "2.0.0-beta.59",
    "vuepress-webpack": "2.0.0-beta.59"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  }
}
