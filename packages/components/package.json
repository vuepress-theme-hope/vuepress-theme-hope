{
  "name": "@mr-hope/vuepress-plugin-components",
  "version": "2.0.0-beta.54",
  "description": "Components plugin for vuepress-theme-hope",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "components"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/components/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/components"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@mr-hope/vuepress-shared": "workspace:*",
    "@vuepress/client": "2.0.0-beta.43",
    "@vuepress/plugin-theme-data": "2.0.0-beta.43",
    "@vuepress/utils": "2.0.0-beta.43",
    "@vueuse/core": "^8.4.2",
    "balloon-css": "^1.2.0",
    "ts-debounce": "^4.0.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15",
    "vuepress-plugin-reading-time2": "workspace:*",
    "vuepress-plugin-sass-palette": "workspace:*"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.43",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "sass-loader": "^12.6.0"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
