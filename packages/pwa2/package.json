{
  "name": "vuepress-plugin-pwa2",
  "version": "2.0.0-beta.242",
  "description": "Powerful PWA plugin for vuepress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "pwa",
    "web app",
    "workbox",
    "service worker"
  ],
  "homepage": "https://plugin-pwa2.vuejs.press/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/pwa2"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./client/*": "./lib/client/*",
    "./package.json": "./package.json"
  },
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.67",
    "@vuepress/shared": "2.0.0-beta.67",
    "@vuepress/utils": "2.0.0-beta.67",
    "@vueuse/core": "^10.5.0",
    "mitt": "^3.0.1",
    "register-service-worker": "^1.7.2",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*",
    "workbox-build": "^7.0.0"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.67"
  },
  "peerDependencies": {
    "sass-loader": "^13.3.2",
    "vuepress": "2.0.0-beta.67",
    "vuepress-vite": "2.0.0-beta.67",
    "vuepress-webpack": "2.0.0-beta.67"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  }
}
