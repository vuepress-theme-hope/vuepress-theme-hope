{
  "name": "vuepress-plugin-pwa2",
  "version": "2.0.0-beta.88",
  "description": "Powerful PWA plugin for vuepress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "pwa",
    "web app",
    "workbox",
    "service worker"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/pwa/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/pwa2"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.49",
    "@vuepress/shared": "2.0.0-beta.49",
    "@vuepress/utils": "2.0.0-beta.49",
    "@vueuse/core": "^9.1.0",
    "mitt": "^3.0.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*",
    "workbox-build": "^6.5.4"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.49",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "sass-loader": "^13.0.0"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
