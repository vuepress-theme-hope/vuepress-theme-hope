{
  "name": "vuepress-plugin-search-pro",
  "version": "2.0.0-rc.53",
  "description": "A advanced search plugin in client",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "search"
  ],
  "homepage": "https://plugin-search-pro.vuejs.press/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/search-pro"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./client/*": "./lib/client/*",
    "./package.json": "./package.json"
  },
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf --glob ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.scss\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@vuepress/helper": "2.0.0-rc.46",
    "@vuepress/plugin-sass-palette": "2.0.0-rc.46",
    "@vueuse/core": "^11.1.0",
    "cheerio": "1.0.0",
    "chokidar": "^3.6.0",
    "slimsearch": "^2.1.1",
    "vue": "^3.5.7",
    "vuepress-shared": "workspace:*"
  },
  "peerDependencies": {
    "sass": "^1.79.2",
    "sass-embedded": "^1.79.2",
    "sass-loader": "^16.0.2",
    "vuepress": "2.0.0-rc.15"
  },
  "peerDependenciesMeta": {
    "sass": {
      "optional": true
    },
    "sass-embedded": {
      "optional": true
    },
    "sass-loader": {
      "optional": true
    }
  },
  "devDependencies": {
    "domhandler": "5.0.3",
    "rollup": "4.22.2"
  },
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=8",
    "pnpm": ">=7",
    "yarn": ">=2"
  }
}
