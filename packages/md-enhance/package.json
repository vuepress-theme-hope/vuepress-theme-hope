{
  "name": "vuepress-plugin-md-enhance",
  "version": "2.0.0-beta.143",
  "description": "Markdown enhancement plugin for vuepress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "gfm",
    "chart",
    "markdown",
    "flowchart",
    "mermaid",
    "tex",
    "katex",
    "slide",
    "presentation",
    "reveal.js"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/md-enhance/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/md-enhance"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client/*": "./lib/client/*",
    "./SlidePage": "./lib/client/SlidePage.js",
    "./package.json": "./package.json"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,css,scss,eot,woff,ttf}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@types/katex": "^0.14.0",
    "@types/markdown-it": "^12.2.3",
    "@vue/repl": "^1.3.2",
    "@vuepress/client": "2.0.0-beta.59",
    "@vuepress/shared": "2.0.0-beta.59",
    "@vuepress/utils": "2.0.0-beta.59",
    "@vueuse/core": "^9.8.1",
    "balloon-css": "^1.2.0",
    "chart.js": "^4.1.1",
    "echarts": "^5.4.1",
    "flowchart.js": "^1.17.1",
    "katex": "^0.16.4",
    "markdown-it": "^13.0.1",
    "mathjax-full": "^3.2.2",
    "mermaid": "^9.3.0",
    "reveal.js": "^4.4.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/babel__core": "7.1.20",
    "@vuepress/bundler-vite": "2.0.0-beta.59",
    "@vuepress/core": "2.0.0-beta.59",
    "@vuepress/markdown": "2.0.0-beta.59",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "sass-loader": "^13.2.0",
    "vuepress": "2.0.0-beta.59",
    "vuepress-vite": "2.0.0-beta.59",
    "vuepress-webpack": "2.0.0-beta.59"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  }
