{
  "name": "vuepress-plugin-copy-code2",
  "version": "2.0.0-rc.1",
  "description": "Copy Code plugin for vuepress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "copy-code",
    "code-copy"
  ],
  "homepage": "https://plugin-copy-code2.vuejs.press/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/copy-code2"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client/*": "./lib/client/*",
    "./package.json": "./package.json"
  },
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=8",
    "pnpm": ">=7",
    "yarn": ">=2"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-rc.0",
    "@vuepress/shared": "2.0.0-rc.0",
    "@vuepress/utils": "2.0.0-rc.0",
    "@vueuse/core": "^10.6.1",
    "balloon-css": "^1.2.0",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-rc.0"
  },
  "peerDependencies": {
    "sass-loader": "^13.3.2",
    "vuepress": "2.0.0-rc.0",
    "vuepress-vite": "2.0.0-rc.0",
    "vuepress-webpack": "2.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  }
}
