{
  "name": "vuepress-plugin-lightgallery",
  "version": "2.0.0-beta.72",
  "description": "Light Gallery plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "lightgallery",
    "image",
    "preview",
    "zoom"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/lightgallery/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/lightgallery"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.46",
    "@vuepress/utils": "2.0.0-beta.46",
    "lightgallery": "^2.4.0",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.46",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "sass-loader": "^13.0.0"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
