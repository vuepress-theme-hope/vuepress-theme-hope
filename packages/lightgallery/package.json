{
  "name": "vuepress-plugin-lightgallery",
  "version": "2.0.0-beta.47",
  "description": "Light Gallery plugin for VuePress",
  "keywords": [
    "vuepress-plugin",
    "vuepress",
    "lightgallery",
    "mr-hope"
  ],
  "homepage": "https://vuepress-theme-hope.github.io/v2/lightgallery/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/lightgallery"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"yarn dev:copy\" \"yarn dev:ts\"",
    "dev:copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "dependencies": {
    "@mr-hope/vuepress-shared": "2.0.0-beta.47",
    "@vuepress/client": "2.0.0-beta.38",
    "@vuepress/utils": "2.0.0-beta.38",
    "lightgallery": "^2.5.0-beta.2",
    "vue": "^3.2.32",
    "vue-router": "^4.0.14",
    "vuepress-plugin-sass-palette": "2.0.0-beta.47"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.43"
  },
  "publishConfig": {
    "access": "public"
  }
}
