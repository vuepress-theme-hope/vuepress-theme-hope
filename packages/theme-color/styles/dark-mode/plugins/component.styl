.theme-dark
  // 分页插件
  .pagation-wrapper
    .pagation-list
      .btn-group
        borderColor()

        div
          &:hover, &.active
            color #000

          &::before
            background-color $nightBordercolor

      input
        bgColor()
        fontColor()
        borderColor()

      .navigateBtn
        borderColor()

        &:hover
          color #000

  // 路径导航
  .breadcrumb ul li.is-active a
    fontColor()

  .tags-wrapper .tag
    background-color #080808

  .back-to-top
    bgColor()