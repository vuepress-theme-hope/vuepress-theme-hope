{
  "name": "vuepress-shared",
  "version": "2.0.0-beta.181",
  "description": "Shared files for vuepress-theme-hope",
  "keywords": [
    "vuepress",
    "vuepress2",
    "utils"
  ],
  "homepage": "https://shared.vuejs.press",
  "bugs": {
    "url": "https://github.comvuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/shared"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./client/*": "./lib/client/*",
    "./node": "./lib/node/index.js",
    "./noopModule": "./lib/client/noopModule.js",
    "./styles/*": "./styles/*",
    "./package.json": "./package.json"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "styles"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.60",
    "@vuepress/shared": "2.0.0-beta.60",
    "@vuepress/utils": "2.0.0-beta.60",
    "cheerio": "1.0.0-rc.12",
    "dayjs": "^1.11.7",
    "execa": "^7.0.0",
    "fflate": "^0.7.4",
    "gray-matter": "^4.0.3",
    "striptags": "^3.2.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/connect": "3.4.35",
    "@vuepress/bundler-vite": "2.0.0-beta.60",
    "@vuepress/bundler-webpack": "2.0.0-beta.60",
    "@vuepress/core": "2.0.0-beta.60",
    "@vuepress/plugin-git": "2.0.0-beta.60",
    "ora": "6.1.2",
    "vite": "4.0.4"
  },
  "peerDependencies": {
    "@vuepress/client": "2.0.0-beta.60",
    "vuepress": "2.0.0-beta.60",
    "vuepress-vite": "2.0.0-beta.60",
    "vuepress-webpack": "2.0.0-beta.60"
  },
  "peerDependenciesMeta": {
    "@vuepress/client": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  }
}
