{
  "name": "vuepress-plugin-auto-catalog",
  "version": "2.0.0-beta.217",
  "description": "Generate catalog for VuePress2",
  "keywords": [
    "vuepress",
    "vuepress2",
    "vuepress-plugin",
    "auto-catalog",
    "catalog",
    "readme"
  ],
  "homepage": "https://plugin-auto-catalog.vuejs.press",
  "bugs": {
    "url": "https://github.com/vuepress-theme-hope/vuepress-theme-hope/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-hope/vuepress-theme-hope.git",
    "directory": "packages/auto-catalog"
  },
  "license": "MIT",
  "author": {
    "email": "mister-hope@outlook.com",
    "name": "Mr.Hope",
    "url": "https://mrhope.site"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./package.json": "./package.json"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "pnpm copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.62",
    "@vuepress/core": "2.0.0-beta.62",
    "@vuepress/shared": "2.0.0-beta.62",
    "@vuepress/utils": "2.0.0-beta.62",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vuepress-plugin-components": "workspace:*",
    "vuepress-plugin-sass-palette": "workspace:*",
    "vuepress-shared": "workspace:*"
  },
  "peerDependencies": {
    "sass-loader": "^13.3.1",
    "vuepress": "2.0.0-beta.62",
    "vuepress-vite": "2.0.0-beta.62",
    "vuepress-webpack": "2.0.0-beta.62"
  },
  "peerDependenciesMeta": {
    "sass-loader": {
      "optional": true
    },
    "vuepress": {
      "optional": true
    },
    "vuepress-vite": {
      "optional": true
    },
    "vuepress-webpack": {
      "optional": true
    }
  }
}
